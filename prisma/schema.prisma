generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model codeforcesProblem {
  id        Int      @id @default(autoincrement())
  contestId Int
  index     String
  name      String?
  type      String?
  points    Int?
  tags      String[]
  rating    Int?

  @@unique([contestId, index])
}
model user{
  email String @unique
  password String
}